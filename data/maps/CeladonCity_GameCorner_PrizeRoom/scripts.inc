CeladonCity_GameCorner_PrizeRoom_EventScript_ChoosePrizeMon_Dynamic:: @ 816CBB2

	//setup variables here for randomizer to replace and
	//MultichoiceList_PrintItemsPrizes to pull from
	setvar VAR_TEMP_3, SPECIES_ABRA
	setvar VAR_TEMP_4, SPECIES_CLEFAIRY
	setvar VAR_TEMP_5, SPECIES_DRATINI
	setvar VAR_TEMP_6, SPECIES_SCYTHER
	setvar VAR_TEMP_7, SPECIES_PORYGON
	setvar VAR_TEMP_8, 120
	setvar VAR_TEMP_9, 750
	setvar VAR_TEMP_A, 2500
	setvar VAR_TEMP_B, 4600
	setvar VAR_TEMP_C, 6500

	//don't need this for any logic reason,
	//but menu is glitchy first load if I remove it?
	//is it initilizing a pointer or something?
	getspeciesname 1, VAR_TEMP_3

	message CeladonCity_GameCorner_PrizeRoom_Text_WhichPrize
	waitmessage

	multichoice 11, 0, MULTICHOICE_GAME_CORNER_POKEMON_PRIZES, FALSE

	switch VAR_RESULT
	case 0, CeladonCity_GameCorner_PrizeRoom_EventScript_Abra_Dynamic
	case 1, CeladonCity_GameCorner_PrizeRoom_EventScript_Clefairy_Dynamic
	case 2, CeladonCity_GameCorner_PrizeRoom_EventScript_DratiniPinsir_Dynamic
	case 3, CeladonCity_GameCorner_PrizeRoom_EventScript_ScytherDratini_Dynamic
	case 4, CeladonCity_GameCorner_PrizeRoom_EventScript_Porygon_Dynamic
	case 5, CeladonCity_GameCorner_PrizeRoom_EventScript_EndPrizeExchange
	case 127, CeladonCity_GameCorner_PrizeRoom_EventScript_EndPrizeExchange
	end

CeladonCity_GameCorner_PrizeRoom_EventScript_Abra_Dynamic::
	copyvar VAR_TEMP_1, VAR_TEMP_3
	copyvar VAR_TEMP_2, VAR_TEMP_8
	goto CeladonCity_GameCorner_PrizeRoom_EventScript_ConfirmPrizeMon_Dynamic
	end

