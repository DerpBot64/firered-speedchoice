	end

CeladonCity_GameCorner_PrizeRoom_EventScript_ConfirmPrizeMon_Dynamic::
	getspeciesname 0, VAR_TEMP_1
	msgbox CeladonCity_GameCorner_PrizeRoom_Text_YouWantPrize, MSGBOX_YESNO
	compare VAR_RESULT, NO
	goto_if_eq CeladonCity_GameCorner_PrizeRoom_EventScript_EndPrizeExchange
	checkcoins VAR_RESULT
	compare VAR_RESULT, VAR_TEMP_2
	goto_if_lt CeladonCity_GameCorner_PrizeRoom_EventScript_NotEnoughCoins
	textcolor 3
	switch VAR_TEMP_1
	case VAR_TEMP_3,	CeladonCity_GameCorner_PrizeRoom_EventScript_GiveAbra_Dynamic
	case VAR_TEMP_4,	CeladonCity_GameCorner_PrizeRoom_EventScript_GiveClefairy_Dynamic
	case VAR_TEMP_5,	CeladonCity_GameCorner_PrizeRoom_EventScript_GiveDratini_Dynamic
	case VAR_TEMP_6,	CeladonCity_GameCorner_PrizeRoom_EventScript_GiveScyther_Dynamic
	case VAR_TEMP_7,	CeladonCity_GameCorner_PrizeRoom_EventScript_GivePorygon_Dynamic
	end


CeladonCity_GameCorner_PrizeRoom_EventScript_GiveAbra_Dynamic::
	givemon VAR_TEMP_1, 7, ITEM_NONE
	goto CeladonCity_GameCorner_PrizeRoom_EventScript_CheckReceivedMon
	end

